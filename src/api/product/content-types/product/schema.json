{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "description": {
      "type": "text"
    },
    "introduction": {
      "type": "component",
      "component": "product.introduction",
      "repeatable": false
    },
    "iconList": {
      "type": "component",
      "component": "product.icon-list",
      "repeatable": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "heroHeading": {
      "type": "component",
      "component": "services.heading-parts",
      "repeatable": false
    },
    "heroParagraph": {
      "type": "text"
    },
    "tabsSectionHeading": {
      "type": "component",
      "component": "services.heading-parts",
      "repeatable": false
    },
    "tabsSectionParagraph": {
      "type": "text"
    },
    "productSectionHeading": {
      "type": "component",
      "component": "services.heading-parts",
      "repeatable": false
    },
    "productSectionParagraph": {
      "type": "text"
    },
    "imageSlider": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "mockupTabs": {
      "type": "component",
      "component": "product.mockup-tab",
      "repeatable": true
    }
  }
}
